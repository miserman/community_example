        }
      ],
      "default": "selected_district",
      "display": []
    },
    {
      "id": "selected_region",
      "states": [
        {
          "condition": [
            {
              "id": "selected_county",
              "type": "",
              "value": "",
              "any": false
            }
          ],
          "value": "selected_county"
        }
      ],
      "default": "selected_district",
      "display": []
    },
    {
      "id": "county_subset",
